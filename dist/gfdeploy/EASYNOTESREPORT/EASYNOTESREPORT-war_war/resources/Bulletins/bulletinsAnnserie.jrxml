	<queryString>
		<![CDATA[select c.codeclasse,
l.matriculeeleve,concat(l.nom,' ',l.prenom) as nomeleve ,l.sexe,l.datenaiss,l.lieunaiss,l.adresse,l.redoublant,l.photo,
cl.codematiere,cl.coefficient,cl.groupe,
ca.periode,ca.moyenne as moyenneclasse,ca.moyennepremier,ca.moyennedernier,ca.tauxreussite,
concat(p.nom,' ' ,p.prenom) as nomprof,
ns1.lanote,
trim3.moyenne as moyenneeleve,trim3.rang as rang_classe_eleve,trim3.moyg1,trim3.moyg2,trim3.moyg3,trim3.rangg1,trim3.rangg2,trim3.rangg3,
nm.notemin,nm.notemax,nm.moyenne as moyenne_note,
sr.rang as rang_eleve,
d.absj,d.absn,d.consigne,d.exclusion,
co.decision,string_agg(distinct case when n.lanote<10 then n.codematiere else null end,','),ns2.lanote as notes2,sm1.moyenne as moyenneeleve1,sm2.moyenne as moyenneeleve2,sm3.moyenne as moyenneeleve3,sm4.moyenne as moyenneeleve4,trim1.moyenne as moyenneelevetrim1,trim2.moyenne as moyenneelevetrim2,sm5.moyenne as moyenneeleve5,sm6.moyenne as moyenneeleve6
from classes c
 inner join listedeseleves l on c.codeclasse=l.codeclasse
 inner join clamatcoeffgrpe cl on c.codeclasse=cl.codeclasse
 inner join matieres m on m.codematiere=cl.codematiere
 inner join calculnotesdesclasses ca on ca.codeclasse=c.codeclasse and ca.periode='TRIM3'
 inner join ensgclamat en on en.codeclasse=c.codeclasse and en.codematiere=m.codematiere
 inner join notesdeselevesseq6 ns2 on ns2.matriculeeleve=l.matriculeeleve and m.codematiere=ns2.codematiere
 inner join notesdeselevesseq5 ns1 on ns1.matriculeeleve=l.matriculeeleve and m.codematiere=ns1.codematiere
 inner join notesdeselevestrim3 n on n.matriculeeleve=l.matriculeeleve
 inner join seq1moyennesrangsdeseleves sm1 on sm1.matriculeeleve=l.matriculeeleve
 inner join seq2moyennesrangsdeseleves sm2 on sm2.matriculeeleve=l.matriculeeleve
 inner join seq3moyennesrangsdeseleves sm3 on sm3.matriculeeleve=l.matriculeeleve
 inner join seq4moyennesrangsdeseleves sm4 on sm4.matriculeeleve=l.matriculeeleve
 inner join seq5moyennesrangsdeseleves sm5 on sm5.matriculeeleve=l.matriculeeleve
 inner join seq6moyennesrangsdeseleves sm6 on sm6.matriculeeleve=l.matriculeeleve
 inner join trim1moyennesrangsdeseleves trim1 on trim1.matriculeeleve=l.matriculeeleve
 inner join trim2moyennesrangsdeseleves trim2 on trim2.matriculeeleve=l.matriculeeleve
 inner join trim3moyennesrangsdeseleves trim3 on trim3.matriculeeleve=l.matriculeeleve
 inner join personnels p on p.matricule=en.matriculeenseignant
 inner join notesminmaxgenparmat nm on nm.codeclasse=c.codeclasse and  m.codematiere=nm.codematiere and nm.periode='TRIM3'
 inner join trim3rangdeselevesparmatiere sr on sr.matriculeeleve=l.matriculeeleve and sr.codeclasse=c.codeclasse and  m.codematiere=sr.codematiere
 inner join disciplinesdeselevestrim3 d on d.matriculeeleve=l.matriculeeleve
 inner join conseildeclassetrim3 co on co.matriculeeleve=l.matriculeeleve
where c.codeclasse = $P{pcodeclasse} and l.codeclasse = $P{pcodeclasse} and cl.codeclasse = $P{pcodeclasse} and ca.codeclasse = $P{pcodeclasse} and en.codeclasse = $P{pcodeclasse} and nm.codeclasse = $P{pcodeclasse} and sr.codeclasse = $P{pcodeclasse} and l.matriculeeleve between $P{pmatricule} and $P{pmatriculeA}
group by l.matriculeeleve,c.codeclasse,l.nom,l.prenom,l.sexe,l.datenaiss,l.lieunaiss,l.adresse,l.redoublant,l.photo,cl.codematiere,cl.coefficient,cl.groupe,
ca.periode,ca.moyenne,ca.moyennepremier,ca.moyennedernier,ca.tauxreussite,ns2.lanote,sm3.moyenne,sm4.moyenne,sm5.moyenne,sm6.moyenne,trim1.moyenne,trim2.moyenne,
en.matriculeenseignant,ns1.lanote,trim3.moyenne,trim3.rang,trim3.moyg1,trim3.moyg2,trim3.moyg3,trim3.rangg1,trim3.rangg2,trim3.rangg3,
nm.notemin,nm.notemax,nm.moyenne,sr.rang,p.nom,p.prenom,d.absj,d.absn,d.consigne,d.exclusion,ns1.lanote,sm1.moyenne,sm2.moyenne,co.decision
order by l.nom,l.matriculeeleve,cl.groupe,cl.codematiere]]>
	</queryString>
