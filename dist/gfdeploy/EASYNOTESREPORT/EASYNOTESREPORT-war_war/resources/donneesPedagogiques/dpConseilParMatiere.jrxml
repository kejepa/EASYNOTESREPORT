	<queryString>
		<![CDATA[select dp.niveaux,dp.typedeclasse,dp.codematiere,dp.periode,sum(dp.eff)as eff,
sum(dp.lpcpt) as lpcpt,sum(dp.lfcpt) as lfcpt,sum(dp.lpcpp) as lpcpp,sum(dp.lfcpp) as lfcpp,sum(dp.hpchc) as hpchc,sum(dp.hfchc) as hfchc,sum(dp.nn) as nn,round(avg(dp.mg),2) as mg,sum(dp.nae) as nae,sum(dp.nsc) as nsc
 from donnespedagogiques dp
  where dp.periode=$P{periode} and dp.codematiere=$P{matiere} and dp.section=$P{section}
  group by dp.codematiere,dp.periode, dp.niveaux,dp.typedeclasse
 order by dp.codematiere,dp.typedeclasse desc]]>
